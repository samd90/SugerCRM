<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar"
        ></include>
    <ScrollView
        android:layout_below="@+id/app_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/scrollView5">

    </ScrollView>

    <LinearLayout
        android:id="@+id/liner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/scrollView5">

        <LinearLayout
            android:id="@+id/liner_1"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/subTitle"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Subject :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_subject"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_2"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_1"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/off"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Status :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_stut"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_3"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_2"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/fax"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Location :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_loct"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_4"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_3"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/web"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Duration :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_durat"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_5"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_4"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/email"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Assigned To :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_assgn"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_6"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_5"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/bill"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Description :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_des"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_7"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_6"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/type"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Created by :"
                    android:textSize="24dp" />

                <TextView
                    android:id="@+id/text_cretd"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_8"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_7"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/type"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Date Created :"
                    android:textSize="24dp" >
                </TextView>

                <TextView
                    android:id="@+id/text_d_crtd"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/liner_9"
            android:layout_width="match_parent"
            android:layout_height="0.0dp"
            android:layout_below="@+id/liner_8"
            android:layout_weight="0.03"
            android:background="@drawable/app_btn_bg_white" >

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/type"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:gravity="right"
                    android:paddingRight="10dp"
                    android:text="Date Modified :"
                    android:textSize="24dp" >
                </TextView>

                <TextView
                    android:id="@+id/text_d_modi"
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0.1"
                    android:paddingLeft="10dp"
                    android:textSize="24dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>